{
  "extends": "./tsconfig-base",
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "typings/telegraf" // Exclude the telegraf typings directory
  ]
}
